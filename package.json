{
  "name": "floating_browser",
  "version": "1.0.0",
  "description": "A floating browser built with Electron that stays on top of other windows.",
  "main": "src/js/main.js",
  "scripts": {
    "package": "electron-packager . floating_browser --overwrite --platform=win32 --arch=ia32,x64 --prune=true --out=release-builds",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "electron",
    "browser",
    "floating",
    "always-on-top"
  ],
  "author": "Mehdi Abdulhafiz",
  "license": "ISC",
  "devDependencies": {
    "electron": "^27.0.2",
    "electron-packager": "^17.1.2",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "electron-capture": "^1.1.3",
    "electron-context-menu": "^3.6.1",
    "electron-pdf-window": "^1.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.4"
  }
}
